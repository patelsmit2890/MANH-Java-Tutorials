<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>2014-10-10_SessionManagement</display-name>
	<servlet>
		<description>
		</description>
		<display-name>BrowserHeader</display-name>
		<servlet-name>BrowserHeader</servlet-name>
		<servlet-class>ma.browserheader.BrowserHeader</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ProcessRequestServlet</display-name>
		<servlet-name>ProcessRequestServlet</servlet-name>
		<servlet-class>ma.controllar.ProcessRequestServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>TransactionServlet</display-name>
		<servlet-name>TransactionServlet</servlet-name>
		<servlet-class>ma.controllar.TransactionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BrowserHeader</servlet-name>
		<url-pattern>/BrowserHeader.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProcessRequestServlet</servlet-name>
		<url-pattern>/proreq</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TransactionServlet</servlet-name>
		<url-pattern>/TransactionServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
