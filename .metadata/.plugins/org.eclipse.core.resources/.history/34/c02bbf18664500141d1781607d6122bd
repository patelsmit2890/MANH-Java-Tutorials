package assertion;

/**
 * @author smpatel
 *
 */
class Arithmatic{
	public int sum(int num1,int num2){
		return num1*num2;
	}
}
public class AssertionExample {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		assert new Arithmatic.sum(2,4)==6;
	}

}
