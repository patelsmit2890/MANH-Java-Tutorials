<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>SSO</display-name>

	<servlet>
		<description>
		</description>
		<display-name>ProcessServlet</display-name>
		<servlet-name>ProcessServlet</servlet-name>
		<servlet-class>web.ProcessServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>TransactionAdmin</display-name>
		<servlet-name>TransactionAdmin</servlet-name>
		<servlet-class>web.TransactionAdmin</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ResourceAssignServlet</display-name>
		<servlet-name>ResourceAssignServlet</servlet-name>
		<servlet-class>web.ResourceAssignServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>AssignRolesServlet</display-name>
		<servlet-name>AssignRolesServlet</servlet-name>
		<servlet-class>web.AssignRolesServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ModifyResorceServlet</display-name>
		<servlet-name>ModifyResorceServlet</servlet-name>
		<servlet-class>web.ModifyResorceServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>AddUsersServlet</display-name>
		<servlet-name>AddUsersServlet</servlet-name>
		<servlet-class>web.AddUsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProcessServlet</servlet-name>
		<url-pattern>/process</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TransactionAdmin</servlet-name>
		<url-pattern>/transact.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ResourceAssignServlet</servlet-name>
		<url-pattern>/assignResource</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AssignRolesServlet</servlet-name>
		<url-pattern>/assignRoles</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ModifyResorceServlet</servlet-name>
		<url-pattern>/modifyResorce</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddUsersServlet</servlet-name>
		<url-pattern>/addUsers</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Login.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
